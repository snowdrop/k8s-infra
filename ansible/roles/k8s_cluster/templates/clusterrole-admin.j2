{% if k8s_version.startswith('1.23') %}
apiVersion: rbac.authorization.k8s.io/v1
{% else %}
apiVersion: rbac.authorization.k8s.io/v1beta1
{% endif %}
kind: ClusterRoleBinding
metadata:
  name: system:default:default
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: cluster-admin
subjects:
  - kind: ServiceAccount
    name: default
    namespace: default
